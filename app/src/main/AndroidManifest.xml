<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ute.tinit.chatkotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Activity_flash_screen">
        </activity>
        <activity android:name=".Activity.activity_profile" />
        <activity android:name=".Activity.activity_profile_more" />
        <activity android:name=".Activity.activity_profile_more_myprofile" />
        <activity android:name=".Activity.activity_profile_more_myprofile_changeprofile" />
        <activity android:name=".Activity.activity_setting" />
        <activity android:name=".Activity.activity_login" />
        <activity android:name=".Activity.activity_new_account" />
        <activity android:name=".Activity.activity_chat_active">

        </activity>
    </application>

</manifest>